{
  "name": "larabase/nova-page",
  "description": "A Laravel Nova tool for editing custom settings using native Nova fields.",
  "keywords": [
    "laravel",
    "nova"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.2.0",
    "laravel/nova": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Larabase\\NovaPage\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Larabase\\NovaPage\\NovaPageServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    },
    {
      "type": "vcs",
      "url": "https://github.com/laravel/nova-dusk-suite"
    }
  ]
}
