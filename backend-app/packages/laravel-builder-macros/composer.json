{
    "name": "larabase/laravel-builder-macros",
    "description": "A set of useful Laravel builder macros.",
    "homepage": "https://github.com/ttungbmt/laravel-builder-macros",
    "license": "MIT",
    "authors": [
        {
            "name": "Truong Thanh Tung",
            "email": "ttungbmt@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "illuminate/database": "^6.0|^7.0|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Larabase\\BuilderMacros\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Larabase\\BuilderMacros\\BuilderMacroServiceProvider"
            ],
            "aliases": {
            }
        }
    }
}
